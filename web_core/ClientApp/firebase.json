{
  "hosting": [
    {
      "public": "./dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "SolutionCore",
      "source": ".",
      "frameworksBackend": {
        "region": "europe-west1"
      }
    }
  ]
}